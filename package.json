{
    "name": "isaisr",
    "version": "0.0.1",
    "description": "Information security and the reliability of information systems",
    "main": "app.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"electron_template\"",
        "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nicklatkovich/ISaISR.git"
    },
    "author": "NickLatkovich",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nicklatkovich/ISaISR/issues"
    },
    "homepage": "https://github.com/nicklatkovich/ISaISR#readme",
    "dependencies": {
        "electron": "^1.7.9"
    },
    "devDependencies": {
        "electron-packager": "^9.1.0"
    }
}